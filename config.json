{
    "local": {
        "hc_interval": 1000,
        "foc_interval": 5000,
        "foc_limit": 30,
        "interface": "ens3",
        "role": "master",
        "configured": false
    },
    "cluster": {
        "floating_ip": "192.168.1.10",
        "nodes": {
            "master": {
                "ip": "127.0.0.1",
                "port": "8443"
            },
            "slave": {
                "ip": "127.0.0.1",
                "port": "8443"
            }
        }
    },
    "health_checks": {
        "icmp": {
            "enabled": true
        },
        "http": {
            "enabled": true,
            "address": "http://www.google.com/"
        }
    }
}